<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <stroke android:color="?colorSecondary" android:width="@dimen/prjStroke"/>
            <corners android:radius="@dimen/prjRadius"/>
        </shape>
    </item>

    <item>
        <shape>
            <stroke android:color="@color/gray_dark" android:width="@dimen/prjStroke"/>
            <corners android:radius="@dimen/prjRadius"/>
        </shape>
    </item>

</selector>